{
  "name": "react-text-mask-hoc",
  "version": "0.10.6",
  "description": "A higher-order text-mask component for React and React Native.",
  "author": "Vlad Zhukov (http://github.com/Vlad-Zhukov)",
  "license": "MIT",
  "repository": "Vlad-Zhukov/react-text-mask-hoc",
  "bugs": {
    "url": "https://github.com/Vlad-Zhukov/react-text-mask-hoc/issues"
  },
  "main": "./dist/react-text-mask-hoc.cjs.js",
  "module": "./dist/react-text-mask-hoc.esm.js",
  "scripts": {
    "build": "rimraf dist/ && cross-env NODE_ENV=production node ./rollup.js",
    "test": "jest",
    "lint": "eslint \"**/*.js\"",
    "prettier": "prettier-eslint \"**/*.js\" --write",
    "prepublishOnly": "yarn lint && yarn build && yarn test"
  },
  "dependencies": {
    "hoist-non-react-statics": "^3.0.1",
    "prop-types": "^15.6.2",
    "react-shallow-equal": "^0.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.5.0",
    "prettier-eslint-cli": "^4.7.1",
    "raf": "^3.4.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-native": "^0.56.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.65.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^3.3.0",
    "text-mask-addons": "^3.8.0",
    "text-mask-core": "^5.1.2"
  },
  "peerDependencies": {
    "react": ">=15"
  },
  "keywords": [
    "react",
    "react-native",
    "text-mask",
    "react-text-mask",
    "react-component",
    "react hoc",
    "hoc",
    "text mask",
    "input mask",
    "string mask",
    "input formatting",
    "text formatting",
    "string formatting"
  ],
  "files": [
    "src",
    "dist",
    "ReactNative.js"
  ],
  "jest": {
    "roots": [
      "<rootDir>/__tests__/"
    ],
    "transformIgnorePatterns": [
      "/node_modules((?!text-mask-core).)*$/"
    ]
  }
}
